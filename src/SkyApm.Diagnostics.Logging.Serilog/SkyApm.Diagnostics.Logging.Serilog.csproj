<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>$(PackagePrefix).Diagnostics.Logging.Serilog Write Serilog events to skyapm.</Description>
		<AssemblyTitle>$(PackagePrefix).Diagnostics.Logging.Serilog</AssemblyTitle>
		<TargetFrameworks>netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
		<AssemblyName>$(PackagePrefix).Diagnostics.Logging.Serilog</AssemblyName>
		<PackageId>$(PackagePrefix).Diagnostics.Logging.Serilog</PackageId>
		<PackageTags>SkyWalking;APM;Diagnostics;Serilog</PackageTags>
		<PackageReleaseNotes>
		</PackageReleaseNotes>
		<RootNamespace>SkyApm.Diagnostics.Logging.Serilog</RootNamespace>
	</PropertyGroup>
	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Serilog" Version="2.11.0" />
	</ItemGroup>
	<ItemGroup>
		<None Include="README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\SkyApm.Core\SkyApm.Core.csproj" />
	</ItemGroup>
</Project>
