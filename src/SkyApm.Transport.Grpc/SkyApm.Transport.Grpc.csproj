        <AssemblyTitle>$(PackagePrefix).Transport.Grpc</AssemblyTitle>
        <AssemblyName>$(PackagePrefix).Transport.Grpc</AssemblyName>
        <PackageId>$(PackagePrefix).Transport.Grpc</PackageId>
        <PackageTags>SkyWalking;Grpc</PackageTags>
